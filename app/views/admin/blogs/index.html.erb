<h2>Blogs</h2>

<% @blogs.each do |blog| %>
	<div class="panel panel-default">
		<div class="panel-heading">
			<h3 class="panel-title"><%= blog.name %></h3>
		</div>
		<div class="panel-body">
			<p><%= blog.content %></p>
		</div>
		<div class="panel-footer">
			
			<%= link_to "Edit", edit_admin_blog_path(blog), class: "btn btn-default" %>
		</div>
	</div>
<% end %>		
				